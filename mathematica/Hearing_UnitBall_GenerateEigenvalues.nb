Cell[CellGroupData[{
Cell[BoxData[{
 RowBox[{
  RowBox[{"ClearAll", "[", "\"\<Global`*\>\"", "]"}], "\n"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"numTarget", "=", "20000"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"lMax", "=", "150"}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"nMax", "=", "150"}], ";"}], "\[IndentingNewLine]"}], "\n", 
 RowBox[{
  RowBox[{"eigenvalues", "=", 
   RowBox[{"Flatten", "[", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"With", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"zeros", "=", 
          RowBox[{"N", "[", 
           RowBox[{"BesselJZero", "[", 
            RowBox[{
             RowBox[{"l", "+", 
              RowBox[{"1", "/", "2"}]}], ",", 
             RowBox[{"Range", "[", "nMax", "]"}]}], "]"}], "]"}]}], "}"}], ",", 
        RowBox[{"ConstantArray", "[", 
         RowBox[{
          RowBox[{"zeros", "^", "2"}], ",", 
          RowBox[{
           RowBox[{"2", " ", "l"}], "+", "1"}]}], "]"}]}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"l", ",", "0", ",", "lMax"}], "}"}]}], "]"}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"lambdaSorted", "=", 
    RowBox[{"Sort", "[", 
     RowBox[{"Flatten", "[", "eigenvalues", "]"}], "]"}]}], ";"}], 
  "\[IndentingNewLine]"}], "\n", 
 RowBox[{"If", "[", 
  RowBox[{
   RowBox[{
    RowBox[{"Length", "[", "lambdaSorted", "]"}], ">=", "numTarget"}], ",", 
   RowBox[{
    RowBox[{"finalEigs", "=", 
     RowBox[{"Take", "[", 
      RowBox[{"lambdaSorted", ",", "numTarget"}], "]"}]}], ";", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{
    "exportPath", 
     "=", "\"\<C:\\\\Users\\\\sulta\\\\git\\\\cone-operator-lab\\\\data\\\\\
eigenvalues\\\\unit_ball_N20000.txt\>\""}], ";", "\[IndentingNewLine]", 
    RowBox[{"Export", "[", 
     RowBox[{"exportPath", ",", "finalEigs", ",", "\"\<Table\>\""}], "]"}], ";",
     "\[IndentingNewLine]", 
    RowBox[{"Print", "[", 
     RowBox[{"\"\<Erfolgreich exportiert. Letzter Eigenwert: \>\"", ",", 
      RowBox[{"Last", "[", "finalEigs", "]"}]}], "]"}], ";"}], ",", 
   RowBox[{
    RowBox[{"Print", "[", 
     RowBox[{"\"\<Nicht genug Eigenwerte gefunden. Erh\[ODoubleDot]he lMax \
oder nMax! Aktuelle Anzahl: \>\"", ",", 
      RowBox[{"Length", "[", "lambdaSorted", "]"}]}], "]"}], ";"}]}], 
